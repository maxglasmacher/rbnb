<%= simple_form_for(@yacht) do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price_per_day %>
  <%= f.input :address %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
<%= f.button :submit, class: "btn-primary" %>
<% end %>
